  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "build:docker": "docker build -t matchstick .",
    "buildonly": "graph build",
    "deploy:alchemy": "graph deploy --node https://subgraphs.alchemy.com/api/subgraphs/deploy --ipfs https://ipfs.satsuma.xyz",
    "codegen": "graph codegen --output-dir src/types/",
    "test": "graph test -d",
    "test:no-tty": "docker run -i --rm --mount type=bind,source=$INIT_CWD,target=/matchstick matchstick",
    "create-local": "graph create aperture/uniswap-v3 --node http://localhost:8020",
    "deploy-local": "graph deploy aperture/uniswap-v3 --debug --ipfs http://localhost:5001 --node http://localhost:8020",
    "watch-local": "graph deploy aperture/uniswap-v3 --watch --debug --node http://localhost:8020/ --ipfs http://localhost:5001",
    "create-remote": "graph create aperture/uniswap-v3 --node http://manta--LoadB-GhHvBdQjQr2X-e4fa964ae140c9e9.elb.us-west-2.amazonaws.com:8020",
    "deploy-remote": "graph deploy aperture/uniswap-v3 --debug --node http://manta--LoadB-GhHvBdQjQr2X-e4fa964ae140c9e9.elb.us-west-2.amazonaws.com:8020 --ipfs http://manta--LoadB-GhHvBdQjQr2X-e4fa964ae140c9e9.elb.us-west-2.amazonaws.com:5001"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.64.1",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@uniswap/eslint-config": "^1.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.1.0",
    "matchstick-as": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
